<UserControl x:Class="TPWSE.ClientApplication.Pages.ParksControl.ParksControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TPWSE.ClientApplication.Pages.ParksControl"
             mc:Ignorable="d" 
             Style="{StaticResource BubbleUXContentElement}" Padding="10" MinWidth="200" Foreground="White">
    <DockPanel>
        <TextBlock Name="TitleBlock"  DockPanel.Dock="Top" HorizontalAlignment="Center" FontSize="16" FontWeight="Bold" Text="Parks"/>
        <Separator DockPanel.Dock="Top" Margin="0,10"/>
        <Frame x:Name="ParksControlContent">
            <Frame.Content>
                <local:ParksSearchPage/>
            </Frame.Content>
        </Frame>
    </DockPanel>
</UserControl>
