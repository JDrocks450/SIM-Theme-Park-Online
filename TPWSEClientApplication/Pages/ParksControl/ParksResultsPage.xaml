<Page x:Class="TPWSE.ClientApplication.Pages.ParksControl.ParksResultsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TPWSE.ClientApplication.Pages.ParksControl"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ParksResultsPage" Foreground="White">
    <StackPanel x:Name="ParksView">
        <StackPanel.Resources>
            <Style TargetType="ContentControl" BasedOn="{StaticResource BubbleUXContentElement}">
                <Setter Property="Margin" Value="5,10,5,0"/>
                <Setter Property="BorderThickness" Value="0,2,0,0"/>
                <Setter Property="Padding" Value="10,5"/>
                <Setter Property="BorderBrush" Value="HotPink"/>
            </Style>
        </StackPanel.Resources>
        <ContentControl Visibility="Collapsed">
            <TextBlock>HelloWorld</TextBlock>
        </ContentControl>
    </StackPanel>
</Page>
