<UserControl x:Class="GSQuazar.Controls.StackableFlyoutWindowControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GSQuazar.Controls" xmlns:pages="clr-namespace:GSQuazar.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid x:Name="WindowsGrid">
        <Grid.Resources>
            <Style TargetType="Frame">
                <Setter Property="VerticalAlignment" Value="Center"/>
                <Setter Property="HorizontalAlignment" Value="Center"/>
                <Setter Property="NavigationUIVisibility" Value="Hidden"/>
            </Style>
        </Grid.Resources>
        <TextBlock x:Name="DEBUGWINDOWS" FontSize="36" Visibility="Hidden" VerticalAlignment="Top" HorizontalAlignment="Left">0</TextBlock>
    </Grid>
</UserControl>
